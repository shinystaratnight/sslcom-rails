-formats = certificate_formats(co)
%li
  %strong{style: "color: blue"} certificate download
%li.category_border.download_certificate
  %table
    %tr
      %td format
      %td instructions
    -formats.each do |k,v|
      %tr
        %td
          =link_to v[0], v[1]
        %td
          =link_to "click", v[2], :target => "_blank"