-title = 'Create New User'
-content_for :title, title
.reseller_registration_profile
  %h3=title
  %p
    Please provide email for the new user.
  =form_for @user, :url =>managed_users_path do |f|
    =f.error_messages
    #stylized.default_form
      .subheading Credentials
      =render :partial => "form", :object => f
    #button_container
      #next_submit=image_submit_tag "register.gif"

-content_for :js do
  :javascript
    $( "#user_role_ids" ).select2();
    $(document).ready(function() {
      $( "#user_role_ids" ).select2();
    });
