-csr=co.csr
-if current_user.is_admin?
  %li
    %strong admin functions
  -u = co.ssl_account.users.last
  %li=remote_login_link(u)
  -if csr.try(:body)
    %li=link_to "view dcv file", csr.dcv_url, target: "_blank"
    %li=link_to "download file #{co.csr.md5_hash}.txt", http_dcv_file_csr_path(csr), target: "_blank"
