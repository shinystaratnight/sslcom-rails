    $.manage_common_name = function() {
      <%
        is_basic = @certificate.is_basic?
        is_evucc = @certificate.is_evucc?
        is_premium_ssl = @certificate.is_premium_ssl?
        is_high_assurance = @certificate.is_high_assurance?
      %>

      var old_common_name = $('#old_common_name').val();
      var current_common_name = $('#common_name').val().toLowerCase();
      $('#common_name').val(current_common_name);

      var is_allowed_wildcard_domain = true;
      var is_valid_domain = $.checkDomain(current_common_name);

      if (current_common_name.includes('*')) {
        <%if is_basic || is_evucc || is_premium_ssl || is_high_assurance%>
          is_allowed_wildcard_domain = false;
        <%end%>
      }

      var is_avail_adds_domains = $('textarea[id$=domains]').length ? true : false;

      if (is_valid_domain && is_allowed_wildcard_domain) {
        if (!is_avail_adds_domains) {
          if (current_common_name != old_common_name) {
            current_common_name == '' ? $('#old_common_name').val('') : $('#old_common_name').val(current_common_name);
          }

          return;
        }

        if ($('#content').find('.inner').children().first().hasClass('flash_message')) {
          $('#content').find('.inner').children().first().remove();
        }

        if (current_common_name != old_common_name) {
          var domain_names = $('textarea[id$=domains]').val().toLowerCase();
          var domain_names_ary = domain_names.split(/\s/);

          if (current_common_name == '') {
            var index = $.inArray(old_common_name, domain_names_ary);
            $('#old_common_name').val('');

            if (index != -1) {
              domain_names_ary.splice(index, 1);
              $('textarea[id$=domains]').val(domain_names_ary.join(' '));

              $.adjustDomainPricing($('textarea[id$=domains]')[0]);
            }
          } else {
            <%if is_premium_ssl%>
              var parsed = psl.parse(current_common_name);
              var root_domain = parsed.domain;
              var diff_domains_ary = [current_common_name];

              $.each(domain_names_ary, function(key, value) {
                var parsed_sub = psl.parse(value);
                if (value != old_common_name && parsed_sub.domain == root_domain) {
                  diff_domains_ary.push(value);
                }
                $('textarea[id$=domains]').val(diff_domains_ary.join(' '));

                $.adjustDomainPricing($('textarea[id$=domains]')[0]);
              });
            <%else%>
              var index = $.inArray(old_common_name, domain_names_ary);
              $('#old_common_name').val(current_common_name);

              if ($.inArray(current_common_name, domain_names_ary) != -1) {
                if (index != -1) {
                  domain_names_ary.splice(index, 1);
                  $('textarea[id$=domains]').val(domain_names_ary.join(' '));

                  $.adjustDomainPricing($('textarea[id$=domains]')[0]);
                }
              } else {
                if (index != -1) {
                  domain_names_ary.splice(index, 1);
                }

                domain_names = current_common_name + ' ' + domain_names_ary.join(' ');
                $('textarea[id$=domains]').val(domain_names);

                $.adjustDomainPricing($('textarea[id$=domains]')[0]);
              }
            <%end%>
          }
        }
      } else {
        if (is_avail_adds_domains) {
          var domain_names = $('textarea[id$=domains]').val().toLowerCase();
          var domain_names_ary = domain_names.split(/\s/);
          var index = $.inArray(old_common_name, domain_names_ary);

          if (index != -1) {
            domain_names_ary.splice(index, 1);
            $('textarea[id$=domains]').val(domain_names_ary.join(' '));

            $.adjustDomainPricing($('textarea[id$=domains]')[0]);
          }
        }

        $('#old_common_name').val('');
        $('#common_name').val('');

        var message_wrap_element = $('<div></div>');
        message_wrap_element.attr('class', 'flash_message error');

        var message_element = $('<span></span>');
        if (!is_valid_domain) {
          message_element.append('The common name is invalid one. Please input valid common name.');
        } else {
          message_element.append('It is not allowed wildcard domain for this certificate order.');
        }
        message_wrap_element.append(message_element);

        var btn_element = $('<small></small>');
        var close_btn_element = $('<div></div>');
        close_btn_element.attr('class', 'close_flash_message');

        var close_sign_element = $('<span></span>');
        close_sign_element.append('X');

        close_btn_element.append(close_sign_element);
        close_btn_element.append('close');

        btn_element.append(close_btn_element);
        message_wrap_element.append(btn_element);

        if ($('#content').find('.inner').children().first().hasClass('flash_message')) {
          $('#content').find('.inner').children().first().remove();
        }
        message_wrap_element.insertBefore($('#content').find('.inner').children().first());

        window.scrollTo(0, 0);
      }
    };

