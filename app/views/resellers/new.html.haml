-content_for :title, "Reseller profile information"
=render :partial => 'index_scripts'
=render(:partial => 'shared/form_progress_indicator', :locals => {:pages=>[Reseller::SIGNUP_PAGES, 1]})
.reseller_registration_profile
  %h3 Reseller Profile Information
  %p Please fill out the required fields below to complete your reseller account registration
  =error_messages_for :reseller
  -form_for @reseller, :url => account_reseller_path do |f|
    #stylized.default_form
      =render :partial => "form", :locals => {:f=>f}
    #button_container
      #next_submit=image_submit_tag "next_bl.gif"
