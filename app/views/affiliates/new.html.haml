-content_for :title, "Affiliate Registration"
=render :partial => 'index_scripts'
.reseller_registration_profile
  %h3 SSL.com Affiliate Registration
  %p Please fill out the required fields below to complete your registration for the SSL.com Affiliate Program
  =error_messages_for :reseller
  =form_for @affiliate, :url => account_reseller_path do |f|
    #stylized.default_form
      =render :partial => "form", :locals => {:f=>f}
    #button_container
      #auto_adjust
        =image_tag "cancel_bl.gif", id: "cancel_reseller_signup"
        =image_submit_tag "next_bl.gif"
